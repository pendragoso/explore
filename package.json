{
  "name": "graph-explorer",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://explore.zonos.com",
  "scripts": {
    "build": "next build",
    "codegen": "graphql-codegen -r dotenv/config -c codegen/codegen.ts",
    "dev:open": "concurrently 'pnpm dev:start' 'pnpm proxy' 'pnpm open'",
    "dev:start": "next dev -H dev.explore.zonos.com -p 5000",
    "dev": "concurrently 'pnpm dev:start' 'pnpm proxy'",
    "lint": "next lint",
    "open:build": "open https://dev.explore.zonos.com:3001",
    "open": "open https://dev.explore.zonos.com:5001",
    "proxy": "pnpm proxy:options --source 5001 --target 5000",
    "proxy:options": "NODE_OPTIONS=--dns-result-order=ipv4first local-ssl-proxy",
    "start": "next start",
    "storybook:build": "storybook build",
    "storybook:dev:open": "concurrently 'yarn storybook' 'yarn storybook:proxy' 'yarn storybook:open'",
    "storybook:dev": "concurrently 'yarn storybook' 'yarn storybook:proxy'",
    "storybook:open": "open https://dev.storybook.explore.zonos.com:6021",
    "storybook:proxy": "pnpm proxy:options --source 6021 --target 6020",
    "storybook": "storybook dev -p 6020 --no-open",
    "test": "vitest run"
  },
  "dependencies": {
    "@dqbd/tiktoken": "^1.0.7",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@floating-ui/react-dom": "^1.3.0",
    "@graphiql/plugin-explorer": "^0.3.1",
    "@graphiql/react": "^0.19.0",
    "@t3-oss/env-core": "^0.6.0",
    "@zonos/amino": "^5.1.45",
    "clipboard": "^2.0.11",
    "eventsource-parser": "^1.0.0",
    "framer-motion": "^10.12.4",
    "graphiql": "^3.0.1",
    "graphql": "^16.8.1",
    "graphql-request": "~5.1.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "next": "13.5.1",
    "pretty-bytes": "^6.1.0",
    "react": "18.2.0",
    "react-data-grid": "7.0.0-beta.34",
    "react-dom": "18.2.0",
    "react-hotkeys-hook": "^4.4.0",
    "react-window": "^1.8.9",
    "styled-components": "^5.3.9",
    "swr": "^2.1.3",
    "uuid": "^9.0.0",
    "zod": "^3.22.3"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^4.0.1",
    "@graphql-codegen/cli": "^3.3.0",
    "@graphql-codegen/introspection": "^3.0.1",
    "@graphql-codegen/typescript": "^3.0.3",
    "@graphql-codegen/typescript-graphql-request": "^4.5.9",
    "@graphql-codegen/typescript-operations": "^3.0.3",
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/blocks": "^7.0.6",
    "@storybook/nextjs": "^7.0.6",
    "@storybook/react": "^7.0.6",
    "@storybook/testing-library": "0.0.14-next.2",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/cors": "^2.8.13",
    "@types/lodash": "^4.14.194",
    "@types/node": "18.15.11",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.7",
    "@types/react-window": "^1.8.5",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "concurrently": "^8.0.1",
    "eslint": "8.37.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-deprecation": "^1.4.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-sort-keys": "^2.3.5",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-typescript-custom-sort-keys": "^1.0.5",
    "prettier": "^2.8.7",
    "storybook": "^7.0.6",
